{
  "name": "@dndnotes/srpc",
  "exports": {
    ".": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.mjs",
      "require": "./dist/shared/index.js",
      "default": "./dist/shared/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js",
      "default": "./dist/client/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/shared/index.d.ts"
      ],
      "client": [
        "./dist/client/index.d.ts"
      ],
      "react": [
        "./dist/react/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify --clean",
    "test": "node --no-warnings --experimental-vm-modules ../../node_modules/.bin/jest -i",
    "dev:docs": "storybook dev -p 6007 --no-open",
    "build:docs": "yarn build-storybook",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.40.0",
    "cookie": "^0.6.0",
    "date-fns": "^3.6.0",
    "mongodb": "^6.7.0",
    "mongoose": "^8.4.0",
    "nanoid": "^5.0.7",
    "notion-client": "^6.16.0",
    "superjson": "^2.2.1",
    "ws": "^8.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@storybook/addon-essentials": "^8.1.4",
    "@storybook/addon-links": "^8.1.4",
    "@storybook/blocks": "^8.1.4",
    "@storybook/manager-api": "^8.1.4",
    "@storybook/nextjs": "^8.1.4",
    "@storybook/theming": "^8.1.4",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.13",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "storybook": "^8.1.4",
    "ts-jest": "^29.1.4",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  }
}
