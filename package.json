{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "deps": "ncu -ws --root --reject eslint",
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo run dev",
    "dev:docs": "turbo run dev:docs",
    "build:docs": "turbo run build:docs",
    "env": "turbo run dev:env",
    "test": "turbo run test",
    "tunnel:discord": "sudo cloudflared tunnel run --url http://localhost:3001 discord-activity",
    "tunnel:web": "sudo cloudflared tunnel run --url http://localhost:3000 dndnotes-web",
    "tunnel": "concurrently \"yarn tunnel:discord\" \"yarn tunnel:web\""
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "concurrently": "^8.2.2",
    "isomorphic-git": "^1.25.10",
    "npm-check-updates": "^16.14.20",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "turbo": "^1.13.3",
    "turbo-ignore": "^1.13.3"
  },
  "packageManager": "bun@1.10.0"
}
