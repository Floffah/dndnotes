{
  "name": "@dndnotes/server",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./appRouter": {
      "types": "./dist/appRouter.d.ts",
      "import": "./dist/appRouter.mjs",
      "require": "./dist/appRouter.js",
      "default": "./dist/appRouter.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "appRouter": [
        "./dist/appRouter.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify --clean",
    "test": "bun test --preload src/tests/setup/mongo.setup.ts --update-snapshots"
  },
  "dependencies": {
    "@discordjs/rest": "^2.3.0",
    "@dndnotes/backend-framework": "workspace:*",
    "@dndnotes/lib": "workspace:*",
    "@dndnotes/models": "workspace:*",
    "@tiptap/core": "^2.4.0",
    "@tiptap/pm": "^2.4.0",
    "cookie": "^0.6.0",
    "crypto-random-string": "^5.0.0",
    "date-fns": "^3.6.0",
    "discord-api-types": "^0.37.86",
    "mongodb": "^6.7.0",
    "mongoose": "^8.4.0",
    "nanoid": "^5.0.7",
    "notion-client": "^6.16.0",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/bun": "^1.1.3",
    "dotenv": "^16.4.5",
    "pubnub": "^8.2.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unique-username-generator": "^1.3.0"
  },
  "peerDependencies": {
    "pubnub": "^8.2.1"
  }
}
